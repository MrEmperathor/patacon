                                Errorm=$?
                                LimiteM
                                let E=$E+1
                            elif [[ $SER = 4 ]]; then
                                aria2c $K
                                let E=$E+1
                            fi


                            printf "TEMPORADA #"; printf "$iii"; echo 

                            function MoverArchivos() {



                                for f in * ; do mv "$f" "${f// /_}" ; done > /dev/null 2>> /root/log.txt

