        netuTV[$i]=$(/var/www/html/panel/inc/include/./netu1.php $ruta_drive_todos/$DD/Temp$NT.$DD/"${netuarray[$i]}")
        sleep 3
        if [[ $i = $b ]]; then
            sleep 10
            mul
        fi
    done
    kill $PIDI1
    echo $esp
    for i in "${netuTV[@]}"; do echo $i; done
    echo $esp
    # echo "${netuTV[@]}"

#-------------REPRODUCTOR GOOGLE DIRECTO-----------------#

    # for (( i = 0; i < "${#array[@]}"; i++ )); do
    #     estenxion=${array[$i]##*.}
    # if [[ $estenxion = mkv ]]; then
    #     arrayStream2_D[$i]=$(echo "${array[$i]/\.mkv/\.txt}")
    # elif [[ $estenxion = mp4 ]]; then
    #     arrayStream2_D[$i]=$(echo "${array[$i]/\.mp4/\.txt}")
    # elif [[ $estenxion = avi ]]; then
    #     arrayStream2_D[$i]=$(echo "${array[$i]/\.avi/\.txt}")
    # fi
    #     rclone copyto "${array[$i]}" stream2_D:/SERIES/$DD/Temp$NT.$DD/"${arrayStream2_D[$i]}" -P
    # done

    # for (( i = 0; i < "${#arrayStream2_D[@]}"; i++ )); do
    #     linkStream_D[$i]=$(rclone link stream2_D:/SERIES/$DD/$Temp.$DD/"${arrayStream2_D[$i]}")
    # done


#----------------------------------------///------------------------------------------

# for (( rvip = 0; rvip < "${#LinkSerieVip[@]}"; rvip++ )); do
#     ReproVip[$rvip]=$(curl --request GET "http://93.104.209.122/panel/inc/include/linkdrivevip.php?id=${LinkSerieVip[$rvip]}" 2>/dev/null)
# done
for (( rvip = 0; rvip < "${#LinkSerieVip[@]}"; rvip++ )); do
    ReproVip[$rvip]=$(/var/www/html/panel/inc/include/./linkdrivevip.php "${LinkSerieVip[$rvip]}" 2>/dev/null)
done

# for (( eei = 0; eei < "${#LinkSerie2[@]}"; eei++ )); do
