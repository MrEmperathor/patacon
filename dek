function MostarEnlaces()
{
    SERVIDOR=$1; LINKS=$2; ERRORS=$3

    if [[ $ERRORS -eq 0 ]]; then
        echo -e "\n "
        echo -e "\033[1;36mSubido correctamente a\033[0m \033[1;34m$SERVIDOR\033[0m"
        echo -e "$LINKS"
        echo -e "\033[1;31m=====================================\033[0m"
    else
        echo -e "\n "
        echo -e "\033[1;36mVIDA TRISTE NO SE SUBIO\033[0m \033[1;34m$SERVIDOR\033[0m :("

    fi
}

function subi1()
{
    vvery=$(/var/www/html/panell/inc/include/./very.php $MiPeli) 
    errorVery=$?
    echo $vvery > $fifo

    MostarEnlaces "Verystream" $vvery $errorVery
}

function fembed()
{
   for MiPeli in `ls *.mp4 *.mkv *.avi *.mpg` ; do echo $MiPeli; done 2>/dev/null
   for im in `ls *.mp4 *.mkv *.avi *.mpg`; do echo $im ; done 2>/dev/null
   ruu=$(pwd)
   script_F=$($SFembed $ruu $im)
   errorFembed=$?
#    Fembed_Link=$(echo $script_F | awk -F "\) \) " '{print $2}')
   echo $script_F > $fifo1

   MostarEnlaces "Fembed" $script_F $errorFembed
}

function netu()
{
    for MiPeli in `ls *.mp4 *.mkv *.avi *.mpg` ; do echo $MiPeli; done 2>/dev/null
    for im in `ls *.mp4 *.mkv *.avi *.mpg`; do echo $im ; done 2>/dev/null
    netuTV=$($SNetu "$im" 2>/dev/null)
        errornetu=$?
       echo $netuTV > $fifo2

    MostarEnlaces "Netu" $netuTV $errornetu
}

function EnlacesMostar()
{
    UrlVerystream=$(< $fifo)
    echo $UrlVerystream
    UrlFembed=$(< $fifo1)
    echo $UrlFembed
    UrlNetu=$(< $fifo2)
    echo $UrlNetu
}
function subida_o_s_s() 
{
    puesto=$(pwd | cut -d "/" -f7)
    fifo="/tmp/fifo${puesto}very"
    [[ -e $fifo ]] && rm $fifo
    fifo1="/tmp/fifo${puesto}fembed"
    [[ -e $fifo1 ]] && rm $fifo1
    fifo2="/tmp/fifo${puesto}netu"
    [[ -e $fifo2 ]] && rm $fifo2

    PP=1

    echo -e "     ---> \033[1;34mSUBIENDO A VERYSTREAM\033[0m     <--- "
    subi1 &
    sleep 2

    # echo -e "     ---> \033[0;34mSUBIENDO A FEMBED\033[0m <--- "
    # fembed &
    # sleep 2

    echo -e "     ---> \033[0;35mSUBIENDO A NETU\033[0m <--- "
    netu &
    sleep 2
}


