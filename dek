# done

for (( eei = 0; eei < "${#LinkSerie2[@]}"; eei++ )); do
    Cdrive[$eei]=$(/var/www/html/panel/inc/include/./linkdrive2s.php "${LinkSerie2[$eei]}" 2>/dev/null)
done



    # for (( eei = 0; eei2 < "${#LinkSerie2[@]}"; eei2++ )); do
