    # done

    VIP_LINK=$(for linkviip in "${ReproVip[@]}"; do echo $linkviip; done)
    D_DRIVE2=$(for D2i in "${LinkSerie2[@]}"; do echo $D2i; done)
    D_STREAM2=$(for D_i in "${Cdrive[@]}"; do echo $D_i; done)
    Vnetu=$(for i in "${netuTV[@]}"; do echo $i; done)
# D_STREAM2_D=$(for D_i_D in "${GDirecto[@]}"; do echo $D_i_D; echo $esp; done)



ProgressBar
msg -ama "Obteniendo enlaces de OUO..."
for (( COuoii = 0; COuoii < "${#LinkSerieDown[@]}"; COuoii++ )); do
    COuo[$COuoii]=$(/var/www/html/panel/inc/include/./acortador.php ${LinkSerieDown[$COuoii]})
    sleep 2
done
kill $PIDI1
