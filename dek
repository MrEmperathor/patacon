function plantilla2() {
    
#sed -i "s%https://%\nhttps://%g" .LinkOpenSerie.$DD.Temp.$NT
#sed -i "s%https://%\nhttps://%g" .LinkMangoSerie.$DD.Temp.$NT
IP=$(cat "/var/local/IP")
DD2=${DD}
