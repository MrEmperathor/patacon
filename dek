                                    until [[ $v = $t ]]; do
                                        echo $t ; sleep 10
                                        echo -e "\033[0;35mTERMINANDO DE SUBIR TEMPORADA $NT\033[0m"
                                        jobs -l
                                        t=$(jobs -l)
                                    done
                                    echo "TEMPORADA $NT SUBIDA CORRECTAMENTE!!"
                                    echo "NOMBRE DE LA SERIE $DD2"
                                    echo "$RUTA4/SERI.$DD2.Temp.$iii.php"
                                    cd $ruta
