urlencode() {
    local LANG=C i c e=''
    for ((i=0;i<${#1};i++)); do
        c=${1:$i:1}
        [[ "$c" =~ [a-zA-Z0-9\.\~\_\-"https://"] ]] || printf -v c '%%%02X' "'$c"
        e+="$c"
    done
    echo "$e"
}

urldecode() {
    # urldecode <string>

    local url_encoded="${1//+/ }"
    printf '%b' "${url_encoded//%/\\x}"
}

function SaltoLine() {

    for esp in `cat $1` ; do
        echo "https://redir.pelishd24.com/red.php?ul=$esp" >> $1_SaltLine
        echo >> $1_SaltLine
    done
    cat $1_SaltLine
    

}
function SaltoLine1() {

    for esp in `cat $1` ; do
        echo $esp >> $1_SaltLine
        echo >> $1_SaltLine
    done
    cat $1_SaltLine

}
function SaltoEmbed(){

    for esp1 in `cat $1` ; do
        echo $esp1 >> $1_Embed
        echo >> $1_Embed
    done
    sed -i "s%/f/%/embed/%g" "$1_Embed"
    cat $1_Embed
}

